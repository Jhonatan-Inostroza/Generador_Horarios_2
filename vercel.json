{
  "version": 2,
  "builds": [
    {
      "src": "Gestion_de_Acceso/Servidor_Flask/Logica.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "requirements": "requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Gestion_de_Acceso/Servidor_Flask/Logica.py"
    }
  ]
}
